{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "Sample"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AAEDatabase": ""
  },
  "Secrets": {
    "JwtSecret": "",
    "JwtAudience": "",
    "JwtIssuer": "",
    "ApiKey": "",
    "BlobAccount": "",
    "HashIdSalt":  ""
  }
}
